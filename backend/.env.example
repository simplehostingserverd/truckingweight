# Server Configuration
PORT=5001
NODE_ENV=development

# Security Keys
# JWT Secret for NextAuth compatibility (generate a strong random string)
JWT_SECRET=your-jwt-secret-here

# Paseto Secret Key (256-bit secure random key)
# Generate with: node -e "console.log(Buffer.from(crypto.randomBytes(32)).toString('base64'));"
PASETO_SECRET_KEY=your-paseto-secret-key-here

# Supabase Configuration
SUPABASE_URL=your-supabase-url
SUPABASE_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Database Configuration
DATABASE_URL=postgresql://username:password@host:port/database

# Logging
LOG_LEVEL=debug
